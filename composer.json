{
    "name": "survos-sites/barcode-demo",
    "description": "A tiny demo of survos/barcode-bundle",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.2",
        "endroid/installer": "^1.5",
        "inspector-apm/inspector-symfony": "^1.8",
        "nette/php-generator": "^4.1",
        "pierstoval/smoke-testing": "dev-main",
        "survos/barcode-bundle": "^2.0",
        "survos/bootstrap-bundle": "^2.0",
        "symfony/asset": "^7.3 | ^8.0",
        "symfony/asset-mapper": "^7.3 | ^8.0",
        "symfony/console": "^7.3",
        "symfony/doctrine-messenger": "^7.3 | ^8.0",
        "symfony/dotenv": "^7.3 | ^8.0",
        "symfony/expression-language": "^7.3 | ^8.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.3 | ^8.0",
        "symfony/framework-bundle": "^7.3 | ^8.0",
        "symfony/http-client": "^7.3 | ^8.0",
        "symfony/intl": "^7.3 | ^8.0",
        "symfony/mailer": "^7.3 | ^8.0",
        "symfony/mime": "^7.3 | ^8.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.3 | ^8.0",
        "symfony/process": "^7.3 | ^8.0",
        "symfony/property-access": "^7.3 | ^8.0",
        "symfony/property-info": "^7.3 | ^8.0",
        "symfony/runtime": "^7.3 | ^8.0",
        "symfony/security-bundle": "^7.3 | ^8.0",
        "symfony/serializer": "^7.3 | ^8.0",
        "symfony/stimulus-bundle": "^2.25",
        "symfony/string": "^7.3 | ^8.0",
        "symfony/translation": "^7.3 | ^8.0",
        "symfony/twig-bundle": "^7.3 | ^8.0",
        "symfony/ux-icons": "^2.20",
        "symfony/validator": "^7.3 | ^8.0",
        "symfony/web-link": "^7.3 | ^8.0",
        "symfony/yaml": "^7.3 | ^8.0",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/intl-extra": "^3.8",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.1",
        "roave/security-advisories": "dev-latest",
        "survos/code-bundle": "*",
        "survos/command-bundle": "^2.0",
        "survos/crawler-bundle": "^2.0",
        "survos/deployment-bundle": "^2.0",
        "symfony/browser-kit": "^7.3 | ^8.0",
        "symfony/css-selector": "^7.3 | ^8.0",
        "symfony/debug-bundle": "^7.3 | ^8.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "^7.3 | ^8.0",
        "symfony/web-profiler-bundle": "^7.3 | ^8.0",
        "zenstruck/browser": "^1.9",
        "zenstruck/console-test": "^1.7"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "zenstruck/class-metadata": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "php bin/console importmap:install"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "coverage": "rm products-test.db -f && bin/console d:sc:update --force --env=test && php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"

    }
}
